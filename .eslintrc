 {
  "extends": [
    "eslint:recommended", 
    "plugin:react/recommended"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "object-curly-spacing": [2, "always"]
  },
  "settings": {
    "react" : {
      "version": "detect"
    }
  }
}