 {
  "extends": [
    "eslint:recommended", 
    "plugin:react/recommended",
    "plugin:jsx-a11y/strict"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "commonjs": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react",
    "jsx-a11y"
  ],
  "rules": {
    "object-curly-spacing": [2, "always"],
    "no-unused-vars":"warn"
  },
  "settings": {
    "react" : {
      "version": "detect"
    }
  }
}